{
	"name": "browser-babel-react-starter",
	"version": "0.1.0",
	"description": "A bare-bones React application implemented as a Bable transpiled bundle for the browser.",
	"main": "main.js",
	"scripts": {
		"start": "browserify main.js -o public/bundle.js"
	},
	"browserify": {
		"transform": [
			"babelify"
		]
	},
	"dependencies": {
		"babel-register": "^6.24.1",
		"browserify": "^14.3.0",
		"react": "^15.5.4",
		"react-dom": "^15.5.4"
	},
	"devDependencies": {
		"babel-core": "^6.24.1",
		"babel-eslint": "^7.2.3",
		"babel-preset-env": "^1.4.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babelify": "^7.3.0",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^14.1.0",
		"eslint-plugin-babel": "^4.1.1",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^4.0.0",
		"eslint-plugin-react": "^6.10.3"
	},
	"author": {
		"name": "Derek Clair",
